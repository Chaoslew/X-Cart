{% if not this.doJSAggregation() %}
{% for file in this.getJSResources() %}
  <script type="text/javascript"
          src="{{ this.getResourceURL(file.url) }}"
          {% if file.async %} async{% endif %}
          {% if file.defer %} defer{% endif %}></script>
{% endfor %}
{% endif %}
