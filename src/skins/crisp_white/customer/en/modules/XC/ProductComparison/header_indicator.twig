{% set disabled = this.getComparedCount() < 2 %}
<div class="header_product-comparison compare-indicator{% if disabled %} disabled {% endif %}">
	<a {% if not disabled %}href="{{ this.getCompareURL() }}" {% endif %} data-target="{{ this.getCompareURL() }}">
		<span class="counter">{{ this.getComparedCount() }}</span>
	</a>
</div>